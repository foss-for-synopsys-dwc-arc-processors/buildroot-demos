#/bin/sh

case "$1" in
  start)
    cd /lib/modules/4.16.0/kernel/drivers/net/wireless/rsi/
    modprobe rsi_sdio.ko
    cd /root
    sleep 1
    ;;
  stop)
    echo -n "..."
    ;;
  restart|reload)
    "$0" stop
    "$0" start
    ;;
  *)
    echo $"Usage: $0 {start|stop|restart}"
    exit 1
esac

exit $?

